{
    "name": "api",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "eslint \"{src,apps,libs,test}/**/*.ts\" && nest start --watch",
        "start:debug": "node --inspect=0.0.0.0:9229 dist/api/main.js || node --inspect=0.0.0.0:9229 dist/main.js",
        "start:dev:debug": "nest start --debug 0.0.0.0:9229 --watch --preserveWatchOutput",
        "start:dev:nodemon": "eslint \"{src,apps,libs,test}/**/*.ts\" && nodemon --watch src --ext ts,js --exec \"node --inspect=0.0.0.0:9229 -r ts-node/register src/api/main.ts\"",
        "start:prod": "node dist/api/main.js || node dist/main.js",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest --passWithNoTests",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "typeorm": "ts-node ./node_modules/typeorm/cli",
        "migration:generate": "node scripts/migration.js generate",
        "migration:create": "node scripts/migration.js create",
        "migration:run": "node scripts/migration.js run",
        "migration:revert": "node scripts/migration.js revert",
        "prepare": "cd .. && husky install .husky"
    },
    "dependencies": {
        "@aws-sdk/client-secrets-manager": "^3.787.0",
        "@azure/identity": "^4.8.0",
        "@azure/keyvault-secrets": "^4.9.0",
        "@clerk/clerk-sdk-node": "^5.1.6",
        "@modelcontextprotocol/sdk": "^1.11.0",
        "@nestjs/common": "^11.0.1",
        "@nestjs/config": "^4.0.1",
        "@nestjs/core": "^11.0.1",
        "@nestjs/cqrs": "^11.0.2",
        "@nestjs/platform-express": "^11.0.1",
        "@nestjs/swagger": "^11.1.5",
        "@nestjs/typeorm": "^11.0.0",
        "@scalar/nestjs-api-reference": "^0.4.11",
        "@sentry/nestjs": "^9.14.0",
        "axios": "^1.9.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "cookie-parser": "^1.4.7",
        "exceljs": "^4.4.0",
        "openai": "^4.97.0",
        "pg": "^8.14.0",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1",
        "typeorm": "^0.3.21"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.2.0",
        "@eslint/js": "^9.18.0",
        "@nestjs/cli": "^11.0.0",
        "@nestjs/schematics": "^11.0.0",
        "@nestjs/testing": "^11.0.1",
        "@swc/cli": "^0.6.0",
        "@swc/core": "^1.10.7",
        "@types/cookie-parser": "^1.4.8",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.10.7",
        "@types/supertest": "^6.0.2",
        "eslint": "^9.18.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.2",
        "globals": "^16.0.0",
        "husky": "^8.0.3",
        "jest": "^29.7.0",
        "prettier": "^3.4.2",
        "source-map-support": "^0.5.21",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.5.2",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.20.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
