{
  "name": "landing",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "preview": "vite preview",
    "dev:ssr": "eslint src/**/*.{ts,tsx} && tsx server.ts",
    "build:ssr:client": "vite build --outDir dist/client",
    "build:ssr:server": "vite build --outDir dist/server --ssr src/entry-server.tsx",
    "build:ssr": "npm run build:ssr:client && npm run build:ssr:server",
    "start": "NODE_ENV=production node dist/server.js",
    "test": "jest --passWithNoTests",
    "prepare": "cd .. && husky install .husky"
  },
  "dependencies": {
    "compression": "^1.8.0",
    "express": "^4.18.2",
    "framer-motion": "^12.10.0",
    "globals": "^15.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sirv": "^2.0.4",
    "vite-plugin-ssr": "^0.4.142"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "globals": "^16.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "postcss": "^8.5.3",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.3.2",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.30.1",
    "vite": "^5.0.8"
  }
}
